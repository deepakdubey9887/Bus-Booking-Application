<table class="table">
  <thead>
    <tr>

      <th scope="col"><strong>First Name</strong></th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      
    </tr>
  </thead>
  <tbody>
        <% @users.each do |user| %>
            <tr>
   
            <td><%=user.first_name %></td>
            <td><%=user.last_name %></td>
            <td><%=user.email %></td> 
                
            </tr>
        <%end%>
    </tbody>

