bus index page

<table>
  <thead>
     <tr>
        <th>bus_name &emsp;</th>
        <th>boarding&emsp;</th>
        <th>destination&emsp;</th>
        <th>No of Seats&emsp;</th> 
        
         <th>Time&emsp;</th>
        <th>Action</th> 
     </tr>

  </thead>
    <tbody>
        <% @buses.each do |bus| %>
            <tr>
                <td><%= bus.bus_name %></td>
                <td><%= bus.boarding %></td>
                <td><%= bus.destination %></td>
                <td><%= bus.no_of_seats %></td>
               
                <td><%= bus.time %></td>
                <td><%# action %></td>    
            </tr>
        <% end %>
    <tbody>
</table>


<h3> <%= link_to "create new bus", new_bus_path %><h3>